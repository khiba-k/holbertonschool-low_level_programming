while (s[i] == 'a' || s[i] == 'A')
str[i] = '4';
while (s[i] == 'e' || s[i] == 'E')
str[i] = '3';
while (s[i] == 'o' || s[i] == 'O')
str[i] = '0';
while (s[i] == 't' || s[i] == 'T')
str[i] = '7';
while (s[i] == 'l' || s[i] == 'L')
str[i] = '1';
}
return (str);
}
