#include "main.h"

/**
 * jack_baur - prototype name
 * void - void function
*/
void jack_bauer(void)
{
int dh;
int sh;
int dm;
int sm;
for (dh = 0 ; dh <= 2 ; dh++)
{
for (sh = 0 ; sh < 10 ; sh++)
{
for (dm = 0 ; dm < 6 ; dm++)
{
for (sm = 0 ; sm < 10 ; sm++)
{
_putchar('0' + dh);
_putchar('0' + sh);
_putchar(':');
_putchar('0' + dm);
_putchar('0' + sm);
_putchar('\n');
if (dh == 2 && sh == 3 && dm == 5 && sm == 9)
{
return 0;
}
}
}
}
}
}
